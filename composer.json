{
    "name": "php-uniter/php-uniter-yii",
    "description": "",
    "keywords": [
        "php-uniter",
        "php-uniter-yii"
    ],
    "homepage": "https://github.com/php-uniter/php-uniter-yii.git",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Eduard Fomin",
            "email": "eduard.fomin@uniter1.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0@dev",
        "php-uniter/php-uniter-requester": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "PhpUniter\\PhpUniterYii\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/php-uniter/php-uniter-assistant.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
